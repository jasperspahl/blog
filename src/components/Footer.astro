---
import Social from "./Social.astro";
const platforms = ["github", "instagram"]
const username = "jasperspahl"
---
<style>
    footer {
        display: flex;
        gap: 1rem;
        margin-top: 2rem;
    }
</style>
<footer>
    {platforms.map(platform => <Social platform={platform} username={username} />)}
</footer>
