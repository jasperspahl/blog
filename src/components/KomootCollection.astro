---
export interface Props {
    collection_id: number;
}
const { collection_id, } = Astro.props
const title = `Komoot Collection ${collection_id}`;
---
<iframe
    src={`https://www.komoot.de/collection/${collection_id}/embed`}
    width="100%"
    height="580"
    scrolling="no"
    title={title}
/>
<style>
    iframe {
        border-radius: 0.5em;
    }
</style>
